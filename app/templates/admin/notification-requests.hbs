<h1>Notification Requests</h1>

<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>ID</th>
      <th>E-mail</th>
    </tr>
  </thead>
  <tbody>
  {{#each model as |notification-request|}}
    <tr>
      <th>{{notification-request.id}}</th>
      <td>{{notification-request.email}}</td>
    </tr>
  {{/each}}
  </tbody>
</table>